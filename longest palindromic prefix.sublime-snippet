	<content><![CDATA[
vi kmp;
void prefix_function(string s){
    int n=s.length();
    kmp.clear();
    kmp.pb(0);
    for(int i=1;i<n;i++){
        int j=kmp[i-1];
        while(j>0&&s[j]!=s[i])
            j=kmp[j-1];
        if(s[i]==s[j])
            j++;
        kmp.pb(j);
    }
}

string largest_palin_prefix(string str){
    string temp = str;
    R(temp);
    str+="?"+temp;
    prefix_function(str);
    return str.substr(0,kmp.back());
}
]]></content>
