struct CustomCompare
{
    bool operator()(const pii& lhs, const pii& rhs)
    {
        if(lhs.ff!=rhs.ff)return lhs.ff < rhs.ff;
        return lhs.ss<rhs.ss;
    }
};
]]></content>
